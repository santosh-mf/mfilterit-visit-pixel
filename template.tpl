___SANDBOXED_JS_FOR_WEB_TEMPLATE___

var setInWindow = require('setInWindow');
var queryPermission = require('queryPermission');
var injectScript = require('injectScript');
const log = require('logToConsole');
var _mfilteritSettings = 'foo.bar';

// required
const mf_package_name = data.mfilterit_tracking_id;
const mf_custom_additional_value = data.mf_additional_value;


const config = {};

config.mfilterit_tracking_id = mf_package_name;
config.mf_additional_value = mf_custom_additional_value;

setInWindow('_mfilteritSettings', config, true);

const url = 'https://script.mfilterit.net/v3/v/client/com.mf_gtm.js';

if (queryPermission('inject_script', url)) {
  injectScript(url, data.gtmOnSuccess, data.gtmOnFailure);
} else {
  log('Script load failed due to error.');
  data.gtmOnFailure();
}


